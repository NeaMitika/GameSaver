{
  "compilerOptions": {
    "target": "ES2021",
    "useDefineForClassFields": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "isolatedModules": true,
    "noUncheckedIndexedAccess": true
  }
}
